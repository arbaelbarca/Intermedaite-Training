        <activity
            android:name=".CaptureCameraActivity"
            android:exported="false" />
        <activity
            android:name=".LocalChaceActivity"
            android:exported="false" />
        <activity
            android:name=".ParentLifeCycleActivity"
            android:exported="false" />
        <activity
            android:name=".ParentLifeCycleFragment"
